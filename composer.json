{
    "name": "uuf6429/rune",
    "type": "library",
    "description": "PHP Rule Engine.",
    "keywords": ["rule", "engine", "expression", "uuf6429"],
    "homepage": "https://github.com/uuf6429/rune",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Sciberras",
            "homepage": "https://christian.sciberras.me"
        }
    ],
    "require": {
        "php": "^7.4 || ^8",
        "ext-json": "*",
        "symfony/expression-language": "^4 || ^5 || ^6",
        "phpdocumentor/reflection-docblock": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6 || ^9",
        "friendsofphp/php-cs-fixer": "^3",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "uuf6429\\Rune\\": "src/Rune",
            "uuf6429\\Rune\\example\\": "example"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "uuf6429\\Rune\\": [
                "test/unit/Rune",
                "test/integration/Rune"
            ]
        }
    }
}
